{
  "name": "lineage-flow",
  "displayName": "lineage-flow",
  "description": "Visualize, create, and manage column-level data lineage diagrams.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:lineage-flow.openLineageViewer",
    "onCommand:lineage-flow.importFromCsv",
    "onCommand:lineage-flow.exportToImage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lineage-flow.openLineageViewer",
        "title": "Lineage Flow: Open Data Lineage Viewer",
        "category": "Lineage Flow"
      },
      {
        "command": "lineage-flow.importFromCsv",
        "title": "Lineage Flow: Import Tables/Columns from CSV",
        "category": "Lineage Flow"
      },
      {
        "command": "lineage-flow.exportToImage",
        "title": "Lineage Flow: Export Diagram",
        "category": "Lineage Flow"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "csv-parse": "^5.6.0",
    "vis-network": "^9.1.9"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReoGoto/lineage-flow.git"
  },
  "license": "MIT"
}
